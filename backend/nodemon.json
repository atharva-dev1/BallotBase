{
    "watch": [
        "*.js",
        "routes/**/*.js",
        "models/**/*.js",
        "middleware/**/*.js"
    ],
    "ext": "js,json",
    "ignore": [
        "node_modules/**",
        "*.test.js"
    ],
    "exec": "node server.js",
    "env": {
        "NODE_ENV": "development"
    },
    "restartable": "rs",
    "verbose": true
}